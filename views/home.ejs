

<br>
<div class="container" style="display: flex; justify-content: space-between;">
  <a href="/crear" style="margin-left: 30px;" class="btn btn-success">Add</a>
  <span style="font-weight: bold;" >Welcome: User</span>
</div>
<br>
<br>    
<div class="container" >
    <table class="table table-dark"  id="table_id">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Lote</th>
            <th scope="col">name</th>
            <th scope="col">price</th>
            <th scope="col">stock</th>
            <th scope="col">date</th>
            <th scope="col">Acciones</th>
          </tr>
        </thead>
        <tbody>
        <% results.forEach(product => { %>
          <tr>
            <th scope="row">  <%= product.id %>  </th>
            <td> <%= product.number %>  </td>
            <td> <%= product.name%> </td>
            <td> <%= product.price%> </td>
            <td> <%= product.stock%> </td>
            <td> <%= product.date%> </td>
            <td> 
                <a  href="/edit/<%=product.id%>" class="btn btn-warning">Edit</a>
                <a  href="/delete/<%=product.id%>" class="btn btn-danger">Delete</a>
            </td>
          </tr>
        <% }) %>
        </tbody>
      </table>
</div>


